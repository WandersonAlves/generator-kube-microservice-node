{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.spec.ts",
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc",
    "build:docker": "docker build . -t <%= projectName %>",
    "build:webpack": "webpack --config webpack.config.js",
    "publish:docker": "docker push <%= projectName %>",
    "format": "yarn prettier -- --config .prettierrc --write 'src/**/*.ts'",
    "format:test": "yarn prettier -- --config .prettierrc -c 'src/**/*.ts'",
    "server": "node dist/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "watch:build": "npm run clean && tsc --watch",
    "watch:server": "nodemon --inspect './dist/index.js' --watch './dist'",
    "release": "standard-version --release-as"
  },
  "license": "ISC",
  "dependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.0.18",
    "@types/redis": "^2.8.6",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^5.0.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "http-status-codes": "^1.3.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "mongodb": "^3.0.4",
    "mongoose": "^5.1.7",
    "morgan": "^1.9.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "redis": "^2.8.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.15.0",
    "typescript": "^2.9.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.6",
    "source-map-support": "^0.5.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run test && pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
